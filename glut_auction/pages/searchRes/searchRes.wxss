.container{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.main{
  position: fixed;
  display: flex;
  align-items: center;
  top: 1%;
  height: 98%;
  width: 100%;
  border-radius: 10rpx;
}
.goods{
  position: relative;
  width: 47%;
  height: 30%;
  margin: 10rpx;
  float: left;
}
.img{
  position: relative;
  height: 73%;
}
.img image{
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
}
.title{
  margin: 5%;
}